generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Idea {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  status      String 
  complexity  String   @default("MEDIUM")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
